@model Models.Cadastros.Modelo

@{
    ViewBag.Title = "Visualizandos dados de um Modelo";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="panel panel-primary">
    <div class="panel-heading">
        Visualizando detalhes de um Modelo
    </div>
    <div class="panel-body">
        @Html.Partial("~/Views/Modelos/_PartialDetailsContentPanel.cshtml",Model)
        </div>
       
    <div class="panel-footer panel-info">
    @Html.ActionLink("Alterar", "Edit", new { id = Model.ModeloId }, new { @class = "btn	btn-info" })
    @Html.ActionLink("Retornar para a listagem", "Index", null, new { @class = "btn	btn-info" })
    </div>
</div> 
@section	ScriptPage	{
    <script type="text/javascript">
        $(document).ready(function () {
            $('li').removeClass("active");
            $('#liModelos').addClass("active");

        });				
        </script> }
